pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
keys={}
lvl=0
t=0
indt=0
dt=1/60
maxt=3
dx=1
dy=1
state=0
inipos={x=8,y=22}

player={
	ind_sprite = 0,
	moving = false,
	evolpos={},
 	pos={x=0,y=0},
 	update = function(this)
 		if this.moving then
 			this.ind_sprite+=30*dt
 		end

 		if this.ind_sprite>=3 or not this.moving then
			this.ind_sprite =0
 		end
 	end,
 	draw = function(this)
 		spr(17+this.ind_sprite,this.pos.x-4,this.pos.y-4)
 	end
}

light = {
	current_sprite = 0,
	update = function(this)
		this.current_sprite+=dt*10

		if(this.current_sprite>=5) then
			this.current_sprite=0
		end
	end,
	draw = function(this)
		spr(81+this.current_sprite,0,16)
		spr(81+this.current_sprite,0,24)
	end
}

function _init()
	player.pos.x=inipos.x
	player.pos.y=inipos.y
end

function _update()

light.update(light)

if state==0 then
 
	for k in all(keys) do
	k.update(k)
	end

	pos={x=0,y=0}
	if btn(0) then
		new_key(66)
		pos.x-=dx
	elseif btn(1) then
		new_key(68)
		pos.x+=dx
	elseif btn(2) then
		new_key(65)
		pos.y-=dy
	elseif btn(3) then
		new_key(67)
		pos.y+=dy
	else
		new_key(-1)
	end

	add(player.evolpos,pos)
	player.moving = false

elseif state==1 then

	if(abs(player.evolpos[indt+1].x)>0 or abs(player.evolpos[indt+1].y)>0) then
		player.moving = true
	else
		player.moving = false
	end

	player.pos.x+=player.evolpos[indt+1].x
	player.pos.y+=player.evolpos[indt+1].y

	s=mget(player.pos.x/8,player.pos.y/8)
	if(fget(s,0)) then
		player.pos.x-=player.evolpos[indt+1].x
		player.pos.y-=player.evolpos[indt+1].y
		player.moving = false
	elseif(fget(s,1)) then
		state=2
	end
end

player.update(player)

indt+=1
t+=dt

if t>maxt and state<2 then
	t=0
	indt=0
	state+=1
	state%=2
	if state==0 then
		player.evolpos={}
		keys={}
	end
	player.pos.x=inipos.x
	player.pos.y=inipos.y
end

end

function _draw()
	cls()
	camera(-30,-30)

	if state<2 then
		map(0,0,0,0,10,7)
		light.draw(light)
		player.draw(player)
	
		if state==0 then
			print("move!",0,50)
			for k in all(keys) do
			 k.draw(k)
			end
			
		else
		
			print("watch",0,50)
		end
	else
		print("you win")
	end
	
	
	drawtimebar(20,50,51)
	
end


function drawtimebar(offsetx,offsety,len)
	pos={x=offsetx,y=offsety}
	width=4
 if state==0 then
		bartime=(1-t/maxt)*len
		rectfill(pos.x+0,pos.y+0,pos.x+bartime,pos.y+width,8)
	elseif state==1 then
		bartime=t/maxt*len
		rectfill(pos.x+0,pos.y+0,pos.x+bartime,pos.y+width,7)
	end
	
	rect(pos.x+0,pos.y+0,pos.x+len,pos.y+width,7)
end



function new_key(key)

	if #keys>0 then
	 if key==keys[#keys].key then
	 	return
	 end
	end

	keyobject={}
	keyobject.key=key
	keyobject.t=0
	keyobject.x=64.5
	keyobject.y=200

	keyobject.firststep = function(this)
	
	 factor = pow(this.t,1/2)
	 this.y=100*(1-factor)+60*factor
	 this.t+=5.0*dt

	 if this.t>=1 then
	  this.update = this.secondstep
	  this.t=0
	 end

	end

	keyobject.secondstep = function(this)
		if keys[#keys].key!=this.key then
			this.update = this.thirdstep	
		end
	end

	keyobject.thirdstep = function(this)
	 factor = pow(this.t,1/2)
	 this.x=64.5*(1-factor)-10*factor
	 this.t+=5.0*dt

	 if this.t>=1 then
	 	del(keys,this)
	 end
	end

	keyobject.update = keyobject.firststep


	keyobject.draw = function(this)
	 spr(this.key,this.x,this.y)
	end

	add(keys,keyobject)

end

function pow(x,a)
    if (a==0) return 1
    if (a<0) x,a=1/x,-a
    local ret,a0,xn=1,flr(a),x
    a-=a0
    while a0>=1 do
    if (a0%2>=1) ret*=xn
    xn,a0=xn*xn,shr(a0,1)
    end
    while a>0 do
    while a<1 do x,a=sqrt(x),a+a end
    ret,a=ret*x,a-1
    end
    return ret
end
